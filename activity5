public class Main {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
	



			

			double sum=375,ind,ina=3,inb=4,inc=5,deca=4,decb=5,decc=6;

			ind=(sum-(deca+decb+decc))/12;

			System.out.println("Salary of A="+((ina*ind)+deca)+"\n"+"Salary of B="+((inb*ind)+decb)+"\n"+"Salary of C"+((inc*ind)+decc)+"\n");

			}
	}
